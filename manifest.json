{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "fr",
  "version": "0.1",
  "icons": {  "16": "icon16.png",
              "32": "icon32.png",
              "64": "icon64.png",
              "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_title__"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_matches": [
        "https://www.letemps.ch/admin/*"
      ],
      "js": [
        "tippy.all.min.js",
        "jquery-3.3.1.min.js",
        "contentScript.js"
      ],
      "css" : [
        "contentStyle.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
  "icon32.png"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "Shortcut to toggle the popup"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "https://lobbywatch.ch/",
    "https://web.tcch.ch/"
  ]
}
